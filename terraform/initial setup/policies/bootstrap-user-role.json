{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"iam:CreateUser",
				"iam:DeleteUser",
				"iam:DeleteAccessKey",
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:GetRole",
				"iam:CreatePolicy",
				"iam:DeletePolicy",
				"iam:GetUser",
				"iam:GetPolicy",
				"iam:GetPolicyVersion",
				"iam:DeletePolicyVersion",
				"iam:CreatePolicyVersion",
				"iam:AttachUserPolicy",
				"iam:DetachUserPolicy",
				"iam:CreateAccessKey",
				"iam:DeleteAccessKey",
				"iam:ListAccessKeys",
				"iam:TagUser",
				"iam:TagPolicy",
				"iam:TagRole",
				"iam:ListGroupsForUser",
				"iam:ListRolePolicies",
				"iam:ListPolicyVersions",
				"iam:ListAttachedUserPolicies",
				"iam:ListAttachedRolePolicies",
				"iam:ListAttachedRolePolicies",
				"iam:ListInstanceProfilesForRole",
				"iam:AttachRolePolicy",
				"iam:DetachRolePolicy",
				"iam:PassRole",
          		"iam:UpdateAssumeRolePolicy"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
			  "s3:GetObject",
			  "s3:PutObject"
			],
			"Resource": "arn:aws:s3:::ctfrush-terraform-state/terraform/state/initial-tfuser.tfstate"
		}
	]
}